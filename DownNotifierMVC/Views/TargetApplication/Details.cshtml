@model IEnumerable<ModelsLayer.Dtos.AppCheckHistoryDto>


@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.URL)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TargetApplication_Id)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IsUp)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ExecuteTime)
                </th>
          
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.URL)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TargetApplication_Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IsUp)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ExecuteTime)
                    </td>
                  
                </tr>
            }
        </tbody>
    </table>

</div>
